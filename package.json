{
  "name": "@osskit/fetch-enhancers",
  "version": "3.0.0",
  "repository": {
    "url": "https://github.com/osskit/fetch-enhancers"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --forceExit --config tests/jest.config.ts",
    "lint:base": "eslint . --ext .ts",
    "lint": "yarn lint:base --fix",
    "format": "prettier --write '**/*.{ts,js,json}'",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@osskit/eslint-config": "^1.0.3",
    "@osskit/prettier-config": "^0.0.1",
    "@types/async-retry": "^1.4.4",
    "@types/jest": "^28.1.3",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.6.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.2",
    "node-fetch": "^3.2.6",
    "npm-link-check": "^4.0.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "tsc": "^2.0.4",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "async-retry": "^1.3.3",
    "lodash.clonedeep": "^4.5.0"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.{ts,js,json}": "prettier --write"
  }
